<Window x:Class="AlphaS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlphaS"
        mc:Ignorable="d"
        Title="Navigator" Height="350" Width="174.118" Loaded="Window_Loaded" Unloaded="Window_Unload" ResizeMode="NoResize">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Height="20" Click="toggleStockList">toggle stock list window</Button>
            <Button Height="20" Click="toggleBasicDailyData">toggle Basic Daily Data window</Button>
            <Button Height="20" Click="toggleDataAnalyzer">toggle Data Analyzer window</Button>
        </StackPanel>
    </Grid>
</Window>
